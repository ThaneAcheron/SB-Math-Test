                                                                                                     sub buttonclicked_D3_10 
         
                                                                                                         Sound.PlayClick()     
                                                                                  answer_m =   Controls.GetTextBoxText (textbox_m1)   
                                                                                  array [10] = answer_m
                                                                                                      If answer_m = D1_10a Then 
                                                                                                          score = score + 1  
                                                                                                                 EndIf
           percentage = score / 10 * 100  
                     
                if  percentage > 49  Then  
                  congrad = 1 
                Else 
                  congrad = 0
               EndIf 
        
            
           GraphicsWindow.Clear()
     
           If congrad = 0 Then 
           GraphicsWindow.DrawText (180 , 10 , "OoooOps!  ")
           GraphicsWindow.DrawText (230 , 30 , "Your percentage") 
           GraphicsWindow.DrawText (240 , 50 , percentage)
           
         ElseIf congrad = 1 then 
           
           GraphicsWindow.DrawText (180 , 10 , "CONGRADULATIONS!")
           GraphicsWindow.DrawText (230 , 30 , "Your percentage") 
           GraphicsWindow.DrawText (240 , 50 , percentage) 
           
           EndIf
           
